<div class="container">  
  <div class="row">
    <div class="col-md-12 col-sm-12 col-xs-12">Room Reservations</div>
  </div>

  <% @rooms.each do |room| %>
  <div class="row">
      <div class="col-md-12">
        <h2><%= room.name %></h2><br>
        <%= room.description %>
        <div>capacity: <%= room.capacity %></div>
      </div>  
      <div id="calendar_<%= room.id %>">
      </div>
  <% end %>
  </div>
</div>